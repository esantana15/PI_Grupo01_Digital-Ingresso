      <div class="carousel-inner">
        <div class="carousel-item active">
          <img class="d-block w-100" src="/images/home/bannerColdplay.png" alt="Primeiro Slide">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/images/home/bannerDualipa.png" alt="Segundo Slide">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/images/home/bannerArtic.png" alt="Segundo Slide">
        </div>
        <div class="carousel-item">
